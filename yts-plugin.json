  searchEndpoint: "list_movies.json?query_term=",
  httpMethod: "GET",
  requestFormat: "application/json",
  responseFormat: "application/json",
  responseSchema: "data.movies",
  resultFields: {
    title: "title",
    dateAdded: "date_uploaded",
